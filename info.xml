<?xml version="1.0"?>
<extension key="uk.co.mjwconsult.fontawesome" type="module">
  <file>fontawesome</file>
  <name>FontAwesome for CiviCRM 4.6</name>
  <description>FontAwesome is available in CiviCRM core from 4.7.  This extension is a backport of FontAwesome so it can be used in CiviCRM 4.6 (and earlier)</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Matthew Wire</author>
    <email>mjw@mjwconsult.co.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/mattwire/uk.co.mjwconsult.fontawesome</url>
    <url desc="Documentation">https://github.com/mattwire/uk.co.mjwconsult.fontawesome</url>
    <url desc="Support">https://github.com/mattwire/uk.co.mjwconsult.fontawesome</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2017-04-11</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>4.6</ver>
  </compatibility>
  <comments>Backport of FontAwesome for CiviCRM 4.6</comments>
  <civix>
    <namespace>CRM/Fontawesome</namespace>
  </civix>
</extension>
